    "import bluetooth\n",
    "nearby_devices = bluetooth.discover_devices(lookup_names=True)\n",
    "print(\"found %d devices\" % len(nearby_devices))\n",
    "\n",
    "for addr, name in nearby_devices:\n",
    "    f = open(\"myfile.txt\", \"w\")\n",
    "    f.write(\" %s - %s\" % (addr, name))\n",
    "    f.close()\n",
    "    print(\" %s - %s\" % (addr, name))"
   ]
  }
